a { color: #4B58A6; }

.nav-pills > .active > a,
.nav-pills > .active > a:hover {
  color: #ffffff;
  background-color: #4B58A6;
}

#mapCanvas img {
  max-width: none;
}

.filter-box { 
  height: 15px; 
  width: 15px;  
  display: inline-block;
  *border-right-width: 2px;
  *border-bottom-width: 2px;
  -webkit-border-radius: 5px;
     -moz-border-radius: 5px;
          border-radius: 5px;
  -webkit-background-clip: padding-box;
     -moz-background-clip: padding;
          background-clip: padding-box; 
}

.filter-cir { 
  height: 10px; 
  width: 10px;  
  display: inline-block;
  *border-right-width: 2px;
  *border-bottom-width: 2px;
  -webkit-border-radius: 10px;
     -moz-border-radius: 10px;
          border-radius: 10px;
  -webkit-background-clip: padding-box;
     -moz-background-clip: padding;
          background-clip: padding-box; 
  margin: 0px 2.5px 0px 2.5px;
}

.filter-yellow { background-color: #FBF358; }
.filter-liquefaction { background-color: #ff9900; }
.filter-landslide { background-color: #274e13; }
.filter-flood { background-color: #23238E; }
.filter-eqintens { background-color: #a64d79; }
.filter-eqfault { background-color: #ff0000; }
.filter-bench { background-color: #7093DB; }

.btn-link,
.btn-link:active {
  background-color: transparent;
  background-image: none;
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;
}
.btn-link {
  border-color: transparent;
  cursor: pointer;
  color: #777;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
}
.btn-link:hover {
  color: #222;
  text-decoration: underline;
  background-color: transparent;
}
.overlay {
    position: absolute;
    top: 60px;
    right: 5px;
	z-index: 10;
}
.overlaychart {
    position: absolute;
    bottom: 0px;
	/*height:220px;
    right: 5px;
	left: 5px;*/
	width: 100%;
	z-index: 10;
}
.well {
	border-color: #777;
	background-color: #fff;
	opacity: 0.95;
	box-shadow: 0 5px 10px rgba(0,0,0,0.5);
}
.wellchart {
	height:200px;
	padding-top: 4px;
	border-color: #777;
	background-color: #fff;
	opacity: 1;
}

.chartheader {
  padding: 4px 4px 4px 10px;
  display: block;
  font-size: 14px;
  font-weight: bold;
  color:#fff;
  background-color: #999;
  border: 1px solid #e1e1e8;
}